{
  "name": "react-tools",
  "description": "A set of complementary tools to React, including the JSX transformer.",
  "version": "0.11.0-alpha",
  "keywords": [
    "react",
    "jsx",
    "transformer",
    "view"
  ],
  "homepage": "http://facebook.github.io/react",
  "bugs": "https://github.com/facebook/react/issues",
  "licenses": [
    {
      "type": "Apache-2.0",
