{
  "name": "react-tools",
  "description": "A set of complementary tools to React, including the JSX transformer.",
  "version": "0.12.0-alpha",
  "keywords": [
    "react",
    "jsx",
    "transformer",
    "view"
  ],
  "homepage": "http://facebook.github.io/react",
