    "coverify": "~1.0.4",
    "envify": "~1.2.0",
    "es3ify": "~0.1.2",
    "es5-shim": "~2.3.0",
    "grunt": "~0.4.2",
    "grunt-cli": "~0.1.9",
    "grunt-compare-size": "~0.4.0",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-compress": "~0.6.0",
    "grunt-contrib-connect": "~0.6.0",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-jshint": "~0.8.0",
    "gzip-js": "~0.3.2",
    "jasmine-tapreporter": "~0.2.2",
    "jest-cli": "~0.1.5",
    "lodash": "~2.4.1",
    "microtime": "~0.5.1",
    "optimist": "~0.6.0",
    "phantomjs": "~1.9",
    "platform": "~1.0.0",
    "populist": "~0.1.6",
    "recast": "~0.5.6",
