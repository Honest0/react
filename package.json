  "devDependencies": {
    "benchmark": "~1.0.0",
    "browserify": "^4.1.10",
    "coverify": "~1.0.4",
    "envify": "^2.0.0",
    "es3ify": "~0.1.2",
    "es5-shim": "^4.0.0",
    "grunt": "~0.4.2",
    "grunt-cli": "~0.1.9",
    "grunt-compare-size": "~0.4.0",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-compress": "^0.9.1",
    "grunt-contrib-connect": "~0.6.0",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-jshint": "^0.10.0",
    "gzip-js": "~0.3.2",
    "jasmine-tapreporter": "~0.2.2",
    "jest-cli": "~0.1.5",
    "lodash": "~2.4.1",
    "microtime": "^0.6.0",
    "optimist": "~0.6.0",
    "phantomjs": "~1.9",
    "platform": "^1.1.0",
    "populist": "~0.1.6",
    "recast": "^0.6.10",
    "sauce-tunnel": "~1.1.0",
    "semver": "^2.3.0",
    "tmp": "~0.0.18",
    "uglify-js": "~2.4.0",
    "uglifyify": "^2.4.0",
    "wd": "~0.2.6"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "preferGlobal": true,
  "commonerConfig": {
    "version": 4
  },
  "scripts": {
    "test": "./node_modules/.bin/grunt test",
    "jest": "jest"
  },
  "jest": {
    "rootDir": "",
    "scriptPreprocessor": "jest/preprocessor.js",
    "setupEnvScriptFile": "jest/environment.js",
    "modulePathIgnorePatterns": ["/build/", "/node_modules/"],
    "testPathIgnorePatterns": ["/build/", "/node_modules/"],
    "unmockedModulePathPatterns": [""]
