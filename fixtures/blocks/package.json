{
  "name": "blocks",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^5.2.0",
    "json-server": "^0.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "prestart": "cp -r ../../build/node_modules/* ./node_modules/",
    "prebuild": "cp -r ../../build/node_modules/* ./node_modules/",
    "start": "concurrently \"npm run start:client\" \"npm run start:api\"",
    "start:api": "json-server --watch db.json --port 3001 --delay 300",
    "start:client": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
