{
  "name": "react-fixtures-ssr",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "concurrently": "3.1.0",
    "http-proxy-middleware": "0.17.3",
    "react-scripts": "0.9.5"
  },
  "dependencies": {
    "express": "^4.14.0",
    "ignore-styles": "^5.0.1",
    "import-export": "^1.0.1",
    "node-fetch": "^1.6.3"
  },
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:client": "NODE_PATH=../../build/packages PORT=3001 react-scripts start",
